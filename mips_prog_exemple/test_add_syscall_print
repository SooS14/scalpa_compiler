        	.data
new_line:  	.asciiz "\n"   # new_line pointe vers "\n\0"
        	.text
        	.globl main

main:
    li     $t0, 4            # 4 dans le registre t0
    mul    $t0, $t0, $t0     # t0*t0 mis dans t0
    li     $t1, 4            # 4 dans le registre t1
    li     $t2, 3            # 3 dans le registre t2
    mul    $t1, $t1, $t2     # 3*4 dans t1
    addu   $t2, $t0, $t1     # t0 + t1 dans t2
    li     $v0, 1	      # utilisation de la primitive print_int
    move   $a0, $t2	      # met la valeur de t2 dans a0 (utilisé par print_int)
    syscall		      # appel système pour lancer la primitive print_int
    li   $v0, 4              # la primitive print_string
    la   $a0, new_line       # a0  l'adresse de new_line
    syscall		      # appel système pour lancer la primitive print_string
    li $v0, 10
    syscall
