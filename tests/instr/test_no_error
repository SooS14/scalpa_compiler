program TEST

var i,j,max : int;
var val : array[-4..4, 0..10] of int;

function f1(t : array[-4..4, 0..10] of int, index : int) : int
    var local_var_1, local_var_2 : int;
    begin
        local_var_1 := index * 7;
        t[index, index] := 6;
        return local_var_1;
    end;

function f2() : int
    var local_var_1, local_var_2 : int;
    begin
        local_var_1 := max;
        return val[i,local_var_1];
    end;

begin
write "hello world";
max := f1(val, i)*5+val[i,j];
max := 2 + f2();
f1(val, val[i,j]);
f2();
end
