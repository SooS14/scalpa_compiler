program maxi
    var g1,g2 : int;
    var g3 : bool;
    var g4 : int;
    

function param_test(p1 : int, p2 : int) : int
   var v1 : int;
   begin
	v1 := p1 + p2;
   	return v1;
   end;

function param_test2(p1 : int, p2 : int) : int
   var v1 : int;
   begin
	v1 := p1 + p2;
   	return v1;
   end;

begin
    g1 := 1;
    g2 := 2;
    g4 := param_test(g1, g2);
    param_test2(g1, g2);
end

