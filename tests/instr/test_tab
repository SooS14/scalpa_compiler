program maxi
	var g1, g2, g3 : int;
	var res : int;
	var val1 : array[0..3] of int;
	var val2 : array[0..2, 0..2, 0..2] of int
    

	function test1(p1 : int, t : array[0..3] of int) : unit
		begin
			write "test1\n";
			write p1;
			write t[0];
			write t[1];
			write t[2];
			write t[3];
			write "\n";
			return;
   		end;


	function test2(t : array[0..2, 0..2, 0..2] of int) : unit
		begin
			write "test2\n";
			write t[0, 0, 0];
			write t[0, 0, 1];
			write t[0, 0, 2];
			write t[0, 1, 0];
			write t[0, 1, 1];
			write t[0, 1, 2];
			write t[0, 2, 0];
			write t[0, 2, 1];
			write t[0, 2, 2];
			write "\n";
					
			return;
   		end;


	function test3(p1 : int, p2 : int, p3 : int) : unit
		begin
			write "test3\n";
			write "p1: ";
			write p1;
			write " p2: ";
			write p2;
			write " p3: ";
			write p3;
			write "\n";
			return;
   		end;
   		
   		
begin
    g1 := 1;
    g2 := 2;
    g3 := 3;
    test1(g1, val1);
    test2(val2);
    test3(g1, g2, g3);
end
